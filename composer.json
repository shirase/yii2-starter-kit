{
    "name": "shirase/yii2-starter-kit",
    "description": "Yii2 Starter Kit Application Template",
    "keywords": [
        "yii2",
        "framework",
        "start",
        "cms",
        "application template",
        "yii2 advanced"
    ],
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.0",
        "ext-intl": "*",
        "ext-mcrypt": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "ext-exif": "*",
        "ext-openssl": "*",
        "ext-gd": "*",
        "ext-curl": "*",
        "yiisoft/yii2": "^2.0.6",
        "yiisoft/yii2-bootstrap": "^2.0.0",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.0.0",
        "yiisoft/yii2-jui": "^2.0.0",
        "asofter/yii2-imperavi-redactor": "^0.0.3",
        "mihaildev/yii2-elfinder": "^1.0",
        "trntv/yii2-aceeditor": "^2.0.2",
        "trntv/probe": "^0.2",
        "trntv/cheatsheet": "^0.1@dev",
        "trntv/yii2-command-bus": "^2.0",
        "intervention/image": "^2.1",
        "vlucas/phpdotenv": "^2.0",
        "bower-asset/admin-lte": "^2.0",
        "bower-asset/font-awesome": "^4.0",
        "bower-asset/html5shiv": "^3.0",
        "bower-asset/jquery-slimscroll": "^1.3",
        "bower-asset/flot": "^0.8",
        "symfony/process": "^3.0",
        "mdmsoft/yii2-admin": "~2.0",
        "voskobovich/yii2-many-many-behavior": "*",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-grid": "^3.1",
        "kartik-v/yii2-detail-view": "^1.7",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/php-date-formatter": "^1.3",
        "kartik-v/yii2-datecontrol": "^1.9",
        "kartik-v/yii2-date-range": "^1.6",
        "kartik-v/yii2-field-range": "^1.3",
        "shirase55/yii2-helpers": "^2.0",
        "shirase55/yii2-sortable": "^1.0",
        "shirase55/yii2-tree-behavior": "^1.0",
        "shirase55/yii2-returned": "^1.0",
        "shirase55/yii2-widgets-activeform": "^1.0",
        "shirase55/yii2-jstree": "^1.0",
        "shirase55/yii2-pjax-ex": "^1.0",
        "shirase55/yii2-glide": "^1.2",
        "shirase55/yii2-file-kit": "^1.2",
        "shirase55/yii2-datetime-widget": "^1.0",
        "2amigos/yii2-ckeditor-widget": "^2.0",
        "insolita/yii2-migration-generator": "^2.2",
        "pheme/yii2-toggle-column": "^0.7.0",
        "iutbay/yii2-kcfinder": "^0.0.1",
        "shirase55/ckeditor-inlinesave": "dev-master",
        "shirase55/ckeditor-image": "dev-master",
        "ckeditor/ckeditor": "^4.6.2"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "^2.0.0",
        "shirase55/yii2-gii": "dev-master",
        "elisdn/yii2-gii-fixture-generator": "*",
        "yiisoft/yii2-codeception": "^2.0.0",
        "yiisoft/yii2-faker": "^2.0.0",
        "codeception/codeception": "^2.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "suggest": {
        "trntv/yii2-debug-xhprof": "dev-master@dev"
    },
    "config": {
        "process-timeout": 1800,
        "preferred-install": "dist",
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            },
            "vcs-driver-options": {
                "github-no-api": true
            },
            "pattern-skip-version": "(-build|-patch|-beta|-dev)"
        }
    }
}
